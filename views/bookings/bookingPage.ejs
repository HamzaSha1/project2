<%- include('../partials/header') %>

<% if(user.role === "Nurse") { %> <%
user.booking.forEach(function(r) { %>
    <p><%= r.customerName %></p>
    <p><%= r.customerEmail %></p>
    <p><%= r.bookingDate.toLocaleDateString() %></p>
    <p><%= r.bookingTime %></p>
    <p><%= r.bookingNotes %></p>
<% }); %>
<% } else if(user.role === 'Customer') { %>
<% user.booking.forEach(function(r) { %>
    <p><%= r.nurseName %></p>
    <p><%= r.nurseEmail %></p>
    <p><%= r.bookingDate.toLocaleDateString() %></p>
    <p><%= r.bookingTime %></p>
    <p><%= r.bookingNotes %></p>

<% }); %> <% } %> <%- include('../partials/footer') %>
